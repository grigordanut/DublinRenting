@model IEnumerable<DublinRenting.Models.Renter>

@{
    ViewBag.Title = "Index";
}

<h2>Renters</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<p>
    @Html.ActionLink("Search", "Search")
</p>

<table class="table" id="newtable">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.RenterID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Last_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone_No)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pref_Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Max_Rent)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.RenterID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.First_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Last_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone_No)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pref_Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Max_Rent)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.RenterID }) |
            @Html.ActionLink("Details", "Details", new { id=item.RenterID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.RenterID })
        </td>
    </tr>
}

</table>
